{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CriteriaResult",
  "type": "object",
  "required": [
    "post_id",
    "p_dx",
    "decision",
    "supporting",
    "conflicts",
    "missing"
  ],
  "properties": {
    "post_id": {
      "type": "string"
    },
    "p_dx": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 1.0
    },
    "decision": {
      "type": "string",
      "enum": [
        "likely",
        "uncertain",
        "unlikely"
      ]
    },
    "supporting": {
      "type": "object",
      "additionalProperties": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "conflicts": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "missing": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "additionalProperties": false
}