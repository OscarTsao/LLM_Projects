{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Predictions",
  "type": "object",
  "required": [
    "post_id",
    "sentence_id",
    "symptom",
    "assertion",
    "score"
  ],
  "properties": {
    "post_id": {
      "type": "string"
    },
    "sentence_id": {
      "type": "string"
    },
    "symptom": {
      "type": "string"
    },
    "assertion": {
      "type": "string",
      "enum": [
        "present",
        "absent"
      ]
    },
    "score": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 1.0
    },
    "gold": {
      "type": "integer",
      "enum": [
        0,
        1
      ]
    }
  },
  "additionalProperties": false
}